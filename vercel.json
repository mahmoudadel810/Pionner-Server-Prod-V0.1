{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "/payments/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST"],
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "cache-control": "no-cache"
      }
    }
  ]
}
